eb|url = https://techcrunch.com/2017/08/24/pishevar-sends-another-letter-to-uber-board-about-benchmark/|title = Pishevar intervenes in Benchmark-Kalanick lawsuit|date = August 24, 2017|accessdate = September 9, 2017|publisher = ''TechCrunch''|last = Roof|first = Katie}}</ref>
